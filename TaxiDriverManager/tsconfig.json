{
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [ "es2015", "dom" ],
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "types": ["webpack-env"]
  },
  "exclude": [
      "bin",
      "node_modules",
      "bower_components"
  ]
}
